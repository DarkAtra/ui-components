{
  "extends": "../../tsconfig.settings.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "es2017"
    ],
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "jsx": "react",
    "jsxFactory": "h",
    "strict": false
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
